{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "client/dist",
          "buildCommand": "npm run build",
          "installCommand": "npm install"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "client/dist/index.html"
      }
    ]
  }
  